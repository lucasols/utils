{
  "name": "@lucasols/utils",
  "description": "Typescript utils",
  "version": "4.18.1",
  "license": "MIT",
  "scripts": {
    "test": "vitest --ui",
    "test:run": "vitest run",
    "lint": "pnpm tsc && pnpm eslint",
    "tsc": "tsc -p tsconfig.prod.json",
    "eslint": "CI=true eslint src/ scripts/ --color --max-warnings=0",
    "build": "pnpm test:run && pnpm lint && pnpm build:no-test && pnpm build:update-exports",
    "build:no-test": "tsup",
    "build:update-exports": "tsm --no-warnings scripts/updatePackageExports.ts",
    "build-test": "tsup --config tsup.test.config.ts",
    "pre-publish": "./scripts/check-if-is-sync.sh && pnpm build"
  },
  "files": [
    "dist"
  ],
  "repository": "github:lucasols/utils",
  "author": "Lucas Santos",
  "type": "module",
  "exports": {
    "./arrayUtils": {
      "import": "./dist/arrayUtils.js",
      "types": "./dist/arrayUtils.d.ts"
    },
    "./assertions": {
      "import": "./dist/assertions.js",
      "types": "./dist/assertions.d.ts"
    },
    "./consoleFmt": {
      "import": "./dist/consoleFmt.js",
      "types": "./dist/consoleFmt.d.ts"
    },
    "./dedent": {
      "import": "./dist/dedent.js",
      "types": "./dist/dedent.d.ts"
    },
    "./deepEqual": {
      "import": "./dist/deepEqual.js",
      "types": "./dist/deepEqual.d.ts"
    },
    "./enhancedMap": {
      "import": "./dist/enhancedMap.js",
      "types": "./dist/enhancedMap.d.ts"
    },
    "./internalUtils": {
      "import": "./dist/internalUtils.js",
      "types": "./dist/internalUtils.d.ts"
    },
    "./objUtils": {
      "import": "./dist/objUtils.js",
      "types": "./dist/objUtils.d.ts"
    },
    "./parallelAsyncResultCalls": {
      "import": "./dist/parallelAsyncResultCalls.js",
      "types": "./dist/parallelAsyncResultCalls.d.ts"
    },
    "./promiseUtils": {
      "import": "./dist/promiseUtils.js",
      "types": "./dist/promiseUtils.d.ts"
    },
    "./rsResult": {
      "import": "./dist/rsResult.js",
      "types": "./dist/rsResult.d.ts"
    },
    "./runShellCmd": {
      "import": "./dist/runShellCmd.js",
      "types": "./dist/runShellCmd.d.ts"
    },
    "./shallowEqual": {
      "import": "./dist/shallowEqual.js",
      "types": "./dist/shallowEqual.d.ts"
    },
    "./sleep": {
      "import": "./dist/sleep.js",
      "types": "./dist/sleep.d.ts"
    },
    "./stringUtils": {
      "import": "./dist/stringUtils.js",
      "types": "./dist/stringUtils.d.ts"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@lucasols/eslint-plugin-extended-lint": "^1.5.0",
    "@types/eslint": "^8.56.2",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/ui": "^1.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vitest": "^0.3.20",
    "prettier": "3.2.4",
    "prettier-plugin-organize-imports": "^3.2.4",
    "tsm": "^2.3.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.1"
  }
}
