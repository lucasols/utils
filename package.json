{
  "name": "@ls-stack/utils-monorepo",
  "private": true,
  "description": "Monorepo for @ls-stack TypeScript utilities",
  "version": "3.18.0",
  "license": "MIT",
  "scripts": {
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "build": "pnpm -r build",
    "build:utils": "pnpm --filter @ls-stack/utils build",
    "build:node-utils": "pnpm --filter @ls-stack/node-utils build",
    "build:browser-utils": "pnpm --filter @ls-stack/browser-utils build",
    "docs": "pnpm -r docs",
    "docs:utils": "pnpm --filter @ls-stack/utils docs",
    "docs:node-utils": "pnpm --filter @ls-stack/node-utils docs",
    "docs:browser-utils": "pnpm --filter @ls-stack/browser-utils docs",
    "docs:watch": "pnpm -r --parallel docs:watch",
    "build-lint-test-all": "pnpm build && pnpm lint && pnpm test"
  },
  "repository": "github:lucasols/utils",
  "author": "Lucas Santos",
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "catalogs": {
      "default": {
        "@eslint/js": "^9.18.0",
        "@ls-stack/extended-lint": "^0.20.1",
        "@types/eslint": "^9.6.1",
        "@types/eslint__js": "^8.42.3",
        "@types/node": "^22.10.9",
        "@typescript-eslint/eslint-plugin": "^8.21.0",
        "@typescript-eslint/parser": "^8.21.0",
        "@vitest/ui": "^3.0.4",
        "dequal": "^2.0.3",
        "eslint": "^9.18.0",
        "eslint-plugin-unicorn": "^56.0.1",
        "eslint-plugin-vitest": "^0.5.4",
        "evtmitter": "^0.3.3",
        "mitata": "^1.0.32",
        "prettier": "3.4.2",
        "prettier-plugin-organize-imports": "^4.1.0",
        "t-result": "^0.3.0",
        "tsm": "^2.3.0",
        "tsup": "^8.3.5",
        "typedoc": "^0.28.4",
        "typedoc-plugin-markdown": "^4.6.3",
        "typedoc-plugin-missing-exports": "^4.0.0",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.21.0",
        "vite": "^6.0.11",
        "vitest": "^3.0.4"
      }
    }
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@ls-stack/extended-lint": "catalog:",
    "@types/eslint": "catalog:",
    "@types/eslint__js": "catalog:",
    "@types/node": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "@vitest/ui": "catalog:",
    "dequal": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-unicorn": "catalog:",
    "eslint-plugin-vitest": "catalog:",
    "mitata": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-organize-imports": "catalog:",
    "tsm": "catalog:",
    "tsup": "catalog:",
    "typedoc": "catalog:",
    "typedoc-plugin-markdown": "catalog:",
    "typedoc-plugin-missing-exports": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  }
}
